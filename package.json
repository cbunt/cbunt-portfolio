  "type": "module",
  "description": "portfolio site for cass bunting",
  "keywords": [
    "portfolio",
    "graphics"
  ],
  "scripts": {
    "build": "pnpm run clean && NODE_OPTIONS=\"--import tsx\" webpack --mode production",
    "build:watch": "pnpm run clean && NODE_OPTIONS=\"--import tsx\" webpack --mode production -w",
    "build:dev": "pnpm run clean && NODE_OPTIONS=\"--import tsx\" webpack --mode development",
    "start": "NODE_OPTIONS=\"--import tsx\" webpack serve --hot --mode development",
    "start:prod": "NODE_OPTIONS=\"--import tsx\" webpack serve --hot --mode production",
    "clean": "rm -fr ./build/*",
    "deploy-pages": "npm run clean && tsx deploy.ts"
  },
